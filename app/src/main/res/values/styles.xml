<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="Body1TextStyle">
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textSize">16sp</item>
    </style>
    <style name="Body2TextStyle">
        <item name="fontFamily">@font/inter_regular</item>
        <item name="android:textSize">14sp</item>
    </style>
    <style name="SoftInputAdjustResizeStyle" parent="Theme.Baspana1">
        <item name="android:windowSoftInputMode">stateVisible|adjustResize</item>
    </style>

    <style name="SoftInputAdjustPanStyle" parent="Theme.Baspana1">
        <item name="android:windowSoftInputMode">adjustPan</item>
    </style>
    <style name="Button1TextStyle">
        <item name="fontFamily">@font/inter_medium</item>
        <item name="android:textSize">14sp</item>
        <item name="textAllCaps">false</item>
        <item name="android:textAlignment">center</item>
        <item name="android:layout_centerVertical">true</item>
        <item name="android:layout_centerInParent">true</item>
    </style>

    <style name="Heading2TextStyle">
        <item name="android:fontFamily">@font/inter_semi_bold</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textSize">23sp</item>
    </style>

    <style name="Header4">
        <item name="fontFamily">@font/inter_semi_bold</item>
        <item name="android:textSize">16dp</item>
        <item name="titleCentered">true</item>
        <item name="color">#000000</item>
    </style>

    <style name="circledShapeableImageView" parent="">
        <item name="cornerFamily">rounded</item>
        <item name="cornerSize">50%</item>
    </style>

    <style name="CollapsingToolbarTextAppearanceCollapsed">
        <item name="android:textAlignment">center</item>
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textSize">16dp</item>
        <item name="android:fontFamily">@font/inter_semi_bold</item>
    </style>

    <style name="CollapsingToolbarTextAppearanceExpanded">
        <item name="android:textColor">@color/colorPrimaryText</item>
        <item name="android:textSize">33dp</item>
        <item name="android:fontFamily">@font/inter_semi_bold</item>
    </style>

    <style name="Subtitle1TextStyle">
        <item name="android:fontFamily">@font/inter_semi_bold</item>
        <item name="android:textSize">16sp</item>
    </style>

    <style name="Subtitle2TextStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">14sp</item>
    </style>

    <style name="Subtitle3TextStyle">
        <item name="android:fontFamily">@font/inter_regular</item>
        <item name="android:textSize">12sp</item>
    </style>
</resources>